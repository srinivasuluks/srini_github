pipeline {
    agent any

    stages {
	    stage('Initial') {
            steps {
                echo 'Hi, Welcome to DevOps..'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}